{
  "name": "Designer",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "precommit": "eslint ./app",
    "android": "node node_modules/react-native/local-cli/cli.js run-android",
    "ios": "node node_modules/react-native/local-cli/cli.js run-ios --simulator 'iPhone 7'",
    "link": "react-native link",
    "bundle-android": "react-native bundle --entry-file index.js --bundle-output ./android/app/src/main/assets/index.android.bundle --assets-dest ./android/app/src/main/res --platform android --dev false",
    "bundle-ios": "react-native bundle --entry-file index.js --bundle-output ./ios/main.jsbundle --assets-dest ./ios --platform ios --dev false",
    "log-android": "react-native log-android",
    "log-ios": "react-native log-ios",
    "push-key-android": "code-push deployment ls doctorz-android -k",
    "push-key-ios": "code-push deployment ls doctorz-ios -k",
    "codepush-android": "code-push release-react doctorz-android android --mandatory true --targetBinaryVersion \"2.2.0\"",
    "codepush-ios": "code-push release-react doctorz-ios ios --mandatory true --targetBinaryVersion \"2.2.0\"",
    "release-android": "cd android && ./gradlew assembleRelease"
  },
  "dependencies": {
    "dva-core": "^1.0.1",
    "is-plain-object": "^2.0.4",
    "moment": "^2.18.1",
    "react": "16.0.0-alpha.12",
    "react-native": "^0.48.1",
    "react-native-calendars": "^1.5.6",
    "react-native-camera": "^0.10.0",
    "react-native-communications": "^2.2.1",
    "react-native-device-info": "^0.11.0",
    "react-native-image-crop-picker": "^0.16.1",
    "react-native-keyboard-aware-scroll-view": "^0.3.0",
    "react-native-linear-gradient": "^2.3.0",
    "react-native-md-textinput": "^2.0.4",
    "react-native-modal-dropdown": "^0.5.0",
    "react-native-parsed-text": "0.0.18",
    "react-native-picker": "^4.2.0",
    "react-native-qrcode": "^0.2.5",
    "react-native-root-toast": "^1.2.0",
    "react-native-roots": "0.0.1",
    "react-native-router-flux": "^4.0.0-beta.17",
    "react-native-scrollable-tab-view": "^0.6.7",
    "react-native-splash-screen": "^3.0.1",
    "react-native-storage": "^0.2.2",
    "react-native-swipeout": "^2.2.2",
    "react-native-swiper": "^1.5.12",
    "react-native-vector-icons": "^4.3.0",
    "react-navigation": "^1.0.0-beta.13",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "redux-logger": "^3.0.6",
    "redux-persist": "^4.9.1",
    "redux-thunk": "^2.2.0",
    "rmc-date-picker": "^5.4.1",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "babel-jest": "21.2.0",
    "babel-preset-react-native": "4.0.0",
    "jest": "21.2.1",
    "react-test-renderer": "16.0.0-beta.5",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "eslint": "^4.6.1",
    "eslint-plugin-prettier": "^2.1.2",
    "eslint-plugin-react": "^7.2.1",
    "prettier": "^1.5.3"
  },
  "jest": {
    "preset": "react-native"
  }
}
